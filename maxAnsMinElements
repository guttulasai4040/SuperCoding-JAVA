class Main{
    public static void main(String[] args){
        int[] arr={1,2,3,4,5,0,-1,20};
        int min=arr[0];
        int max=arr[0];
        
        for(int nums: arr){
            if(nums< min){
                min=nums;
            }
        }
        
         for(int nums: arr){
            if(nums> min){
                max=nums;
            }
        }
        
        System.out.println("min element: "+ min);
        System.out.println("max element: "+ max);
    }
}

public class Main {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5};
        int value = 3;
        boolean contains = false;
        
        for (int num : arr) {
            if (num == value) {
                contains = true;
                break;
            }
        }
        
        System.out.println("Contains value " + value + ": " + contains);
    }
}
